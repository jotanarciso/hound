{
  "name": "cirneco",
  "displayName": "Cirne.co",
  "description": "Generate beautiful code snippets as PNG images",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cirneco.generateSnippet",
        "title": "Cirne.co: Generate with default theme"
      },
      {
        "command": "cirneco.generateSnippetWithTheme",
        "title": "Cirne.co: Generate with theme picker"
      }
    ],
    "keybindings": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run copy-themes",
    "copy-themes": "cp -r src/themes out/ && cp src/themes.json out/",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/prismjs": "^1.26.5",
    "fs-extra": "^11.0.0",
    "prismjs": "^1.30.0",
    "puppeteer": "^21.0.0"
  }
}
